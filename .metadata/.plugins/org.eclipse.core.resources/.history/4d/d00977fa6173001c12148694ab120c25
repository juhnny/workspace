
public class Ex01_Print {

		public static void main(String[] args) {
			
			//출력기능(함수, 자바에서는 Method)을 가진 객체에게 출력 요청
			//1. print()
			System.out.print("Hello");
			System.out.print("\n");
			System.out.print("Nice\n");
			System.out.print('A');
			System.out.print(11);
			System.out.print(1.1);
		
			
		}
}
